{
    "name": "umkm",
    "version": "1.0.0",
    "description": "Surotaste - Full Stack Application",
    "private": true,
    "scripts": {
        "install:all": "npm install --prefix Client && npm install --prefix Server",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --kill-others --names \"SERVER,CLIENT\" --prefix-colors \"bgBlue.bold,bgGreen.bold\"",
        "dev:client": "npm run dev --prefix Client",
        "dev:server": "npm run dev --prefix Server",
        "start": "npm run start:server",
        "start:server": "npm run start --prefix Server",
        "build:client": "npm run build --prefix Client"
    },
    "keywords": [
        "surotaste",
        "react",
        "express",
        "fullstack"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}